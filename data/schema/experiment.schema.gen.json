{
  "$defs": {
    "AdapterModel": {
      "additionalProperties": true,
      "properties": {
        "module_path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Module Path"
        },
        "adapter_name": {
          "default": null,
          "description": "this this is to use inbuilt adapter (e.g. 'simple-assignment-adapter'",
          "title": "Adapter Name",
          "type": "string"
        },
        "config": {
          "title": "Config",
          "type": "object"
        },
        "methods": {
          "title": "Methods",
          "type": "object"
        },
        "note": {
          "default": null,
          "description": "A note for this adapter",
          "title": "Note",
          "type": "string"
        }
      },
      "title": "AdapterModel",
      "type": "object"
    },
    "AggregationModel": {
      "properties": {
        "module_path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Module Path"
        },
        "aggregator_name": {
          "default": null,
          "description": "this this is to use inbuilt aggregator (e.g. 'simple-assignment-adapter'",
          "title": "Aggregator Name",
          "type": "string"
        },
        "config": {
          "anyOf": [
            {
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "title": "Config"
        },
        "note": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Note"
        }
      },
      "title": "AggregationModel",
      "type": "object"
    },
    "ExperimentActivityData": {
      "description": "This is the dataclass for the activities in the experiment.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "config": {
          "description": "setup data (id, outputs, ... arbitrary data",
          "title": "Config"
        },
        "adapter": {
          "description": "The adapter to be used",
          "title": "Adapter",
          "type": "string"
        }
      },
      "required": [
        "name",
        "config",
        "adapter"
      ],
      "title": "ExperimentActivityData",
      "type": "object"
    },
    "ExperimentConfig": {
      "additionalProperties": false,
      "properties": {
        "warn_default_demand": {
          "default": true,
          "title": "Warn Default Demand",
          "type": "boolean"
        },
        "auto_aggregate": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": true,
          "title": "Auto Aggregate"
        },
        "run_adapters_concurrently": {
          "default": true,
          "title": "Run Adapters Concurrently",
          "type": "boolean"
        },
        "run_scenarios": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Run Scenarios"
        },
        "base_directory": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Base Directory"
        },
        "debug_test_is_valid": {
          "default": true,
          "title": "Debug Test Is Valid",
          "type": "boolean"
        },
        "debug_test_replace_bw_config": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "default": true,
          "title": "Debug Test Replace Bw Config"
        },
        "debug_test_expected_error_code": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Debug Test Expected Error Code"
        },
        "debug_test_run": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "title": "Debug Test Run"
        },
        "note": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Note"
        }
      },
      "title": "ExperimentConfig",
      "type": "object"
    },
    "ExperimentHierarchyNodeData": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "aggregator": {
          "description": "name or node-indicator of the aggregator",
          "title": "Aggregator",
          "type": "string"
        },
        "config": {
          "anyOf": [
            {},
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "setup data (id, outputs, ... arbitrary data",
          "title": "Config"
        },
        "children": {
          "anyOf": [
            {
              "items": {
                "anyOf": [
                  {
                    "$ref": "#/$defs/ExperimentHierarchyNodeData"
                  },
                  {
                    "$ref": "#/$defs/ExperimentActivityData"
                  }
                ]
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Children"
        }
      },
      "required": [
        "name",
        "aggregator"
      ],
      "title": "ExperimentHierarchyNodeData",
      "type": "object"
    },
    "ExperimentScenarioData": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "nodes": {
          "additionalProperties": {
            "$ref": "#/$defs/NodeOutput"
          },
          "default": null,
          "description": "name to output, null means default-output (check exists)",
          "title": "Nodes",
          "type": "object"
        },
        "config": {
          "anyOf": [
            {
              "$ref": "#/$defs/ScenarioConfig"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "title": "ExperimentScenarioData",
      "type": "object"
    },
    "NodeOutput": {
      "additionalProperties": false,
      "properties": {
        "unit": {
          "title": "Unit",
          "type": "string"
        },
        "magnitude": {
          "default": 1.0,
          "title": "Magnitude",
          "type": "number"
        }
      },
      "required": [
        "unit"
      ],
      "title": "NodeOutput",
      "type": "object"
    },
    "ScenarioConfig": {
      "properties": {
        "exclude_defaults": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "title": "Exclude Defaults"
        }
      },
      "title": "ScenarioConfig",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "description": "This class is used to store the data of an experiment.",
  "properties": {
    "adapters": {
      "description": "The adapters to be used",
      "items": {
        "$ref": "#/$defs/AdapterModel"
      },
      "title": "Adapters",
      "type": "array"
    },
    "aggregators": {
      "default": [],
      "description": "The aggregators to be used",
      "items": {
        "$ref": "#/$defs/AggregationModel"
      },
      "title": "Aggregators",
      "type": "array"
    },
    "hierarchy": {
      "anyOf": [
        {
          "$ref": "#/$defs/ExperimentHierarchyNodeData"
        },
        {
          "type": "string"
        },
        {
          "format": "path",
          "type": "string"
        }
      ],
      "description": "The activity hierarchy to be used in the experiment",
      "title": "Hierarchy"
    },
    "scenarios": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/ExperimentScenarioData"
          },
          "type": "array"
        },
        {
          "type": "string"
        },
        {
          "format": "path",
          "type": "string"
        }
      ],
      "default": null,
      "description": "The scenarios for this experiment",
      "title": "Scenarios"
    },
    "config": {
      "allOf": [
        {
          "$ref": "#/$defs/ExperimentConfig"
        }
      ],
      "description": "The configuration of this experiment"
    }
  },
  "required": [
    "adapters",
    "hierarchy"
  ],
  "title": "ExperimentData",
  "type": "object"
}