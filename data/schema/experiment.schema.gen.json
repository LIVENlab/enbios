{
  "$defs": {
    "ActivityOutput": {
      "properties": {
        "unit": {
          "title": "Unit",
          "type": "string"
        },
        "magnitude": {
          "default": 1.0,
          "title": "Magnitude",
          "type": "number"
        }
      },
      "required": [
        "unit"
      ],
      "title": "ActivityOutput",
      "type": "object"
    },
    "AdapterModel": {
      "properties": {
        "module_path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Module Path"
        },
        "config": {
          "anyOf": [
            {
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "title": "Config"
        },
        "methods": {
          "anyOf": [
            {
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Methods"
        }
      },
      "title": "AdapterModel",
      "type": "object"
    },
    "AggregationModel": {
      "properties": {
        "module_path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "path",
              "type": "string"
            }
          ],
          "title": "Module Path"
        },
        "config": {
          "anyOf": [
            {
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "title": "Config"
        }
      },
      "required": [
        "module_path"
      ],
      "title": "AggregationModel",
      "type": "object"
    },
    "ExperimentActivityData": {
      "description": "\n    This is the dataclass for the activities in the experiment.\n    the id, is\n    ",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "id": {
          "description": "The identifies (method to find) an activity",
          "title": "Id"
        },
        "adapter": {
          "description": "The adapter to be used",
          "title": "Adapter",
          "type": "string"
        },
        "output": {
          "anyOf": [
            {
              "$ref": "#/$defs/ActivityOutput"
            },
            {
              "maxItems": 2,
              "minItems": 2,
              "prefixItems": [
                {
                  "type": "string"
                },
                {
                  "type": "number"
                }
              ],
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "The default output of the activity",
          "title": "Output"
        },
        "orig_id": {
          "anyOf": [
            {
              "$ref": "#/$defs/ExperimentActivityId"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Temporary copy of the id"
        }
      },
      "required": [
        "name",
        "id",
        "adapter"
      ],
      "title": "ExperimentActivityData",
      "type": "object"
    },
    "ExperimentActivityId": {
      "properties": {
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "database": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Database"
        },
        "code": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Code"
        },
        "location": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Location"
        },
        "unit": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Unit"
        },
        "alias": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Alias"
        }
      },
      "title": "ExperimentActivityId",
      "type": "object"
    },
    "ExperimentConfig": {
      "additionalProperties": false,
      "properties": {
        "warn_default_demand": {
          "default": true,
          "title": "Warn Default Demand",
          "type": "boolean"
        },
        "auto_aggregate": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": true,
          "title": "Auto Aggregate"
        },
        "store_raw_results": {
          "default": false,
          "title": "Store Raw Results",
          "type": "boolean"
        },
        "run_scenarios": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Run Scenarios"
        },
        "base_directory": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "format": "path",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Base Directory"
        },
        "debug_test_is_valid": {
          "default": true,
          "title": "Debug Test Is Valid",
          "type": "boolean"
        },
        "debug_test_replace_bw_config": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "default": true,
          "title": "Debug Test Replace Bw Config"
        },
        "debug_test_expected_error_code": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Debug Test Expected Error Code"
        },
        "debug_test_run": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "title": "Debug Test Run"
        }
      },
      "title": "ExperimentConfig",
      "type": "object"
    },
    "ExperimentHierarchyNodeData": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "aggregator": {
          "title": "Aggregator",
          "type": "string"
        },
        "children": {
          "anyOf": [
            {
              "items": {
                "anyOf": [
                  {
                    "$ref": "#/$defs/ExperimentHierarchyNodeData"
                  },
                  {
                    "$ref": "#/$defs/ExperimentActivityData"
                  }
                ]
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Children"
        }
      },
      "required": [
        "name",
        "aggregator"
      ],
      "title": "ExperimentHierarchyNodeData",
      "type": "object"
    },
    "ExperimentScenarioData": {
      "additionalProperties": false,
      "properties": {
        "activities": {
          "anyOf": [
            {
              "additionalProperties": {
                "anyOf": [
                  {
                    "$ref": "#/$defs/ActivityOutput"
                  },
                  {
                    "maxItems": 2,
                    "minItems": 2,
                    "prefixItems": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "number"
                      }
                    ],
                    "type": "array"
                  }
                ]
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": {},
          "title": "Activities"
        },
        "methods": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Methods"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "config": {
          "anyOf": [
            {
              "$ref": "#/$defs/ScenarioConfig"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "title": "ExperimentScenarioData",
      "type": "object"
    },
    "ScenarioConfig": {
      "properties": {
        "exclude_defaults": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "title": "Exclude Defaults"
        }
      },
      "title": "ScenarioConfig",
      "type": "object"
    }
  },
  "additionalProperties": false,
  "description": "\n    This class is used to store the data of an experiment.\n    ",
  "properties": {
    "adapters": {
      "description": "The adapters to be used",
      "items": {
        "$ref": "#/$defs/AdapterModel"
      },
      "title": "Adapters",
      "type": "array"
    },
    "aggregators": {
      "default": [],
      "description": "The aggregators to be used",
      "items": {
        "$ref": "#/$defs/AggregationModel"
      },
      "title": "Aggregators",
      "type": "array"
    },
    "hierarchy": {
      "anyOf": [
        {
          "$ref": "#/$defs/ExperimentHierarchyNodeData"
        },
        {
          "type": "string"
        },
        {
          "format": "path",
          "type": "string"
        }
      ],
      "default": null,
      "description": "The activity hierarchy to be used in the experiment",
      "title": "Hierarchy"
    },
    "scenarios": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/ExperimentScenarioData"
          },
          "type": "array"
        },
        {
          "type": "string"
        },
        {
          "format": "path",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "The scenarios for this experiment",
      "title": "Scenarios"
    },
    "config": {
      "allOf": [
        {
          "$ref": "#/$defs/ExperimentConfig"
        }
      ],
      "description": "The configuration of this experiment"
    }
  },
  "required": [
    "adapters"
  ],
  "title": "ExperimentData",
  "type": "object"
}